{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Order Array",
  "description": "A order array",
  "type": "array",
  "items": {
    "title": "Order",
    "type": "object",
    "properties": {
      "order_no": {
        "type": "string",
        "description": "",
        "requiredErrorCode": "订单号必须"
      },
      "amount": {
        "description": "Name of the product",
        "type": "number",
        "minimum": 1000,
        "exclusiveMinimum": true,
        "typeErrorCode": "ajsdjkasdjkas",
        "minimumErrorCode": "xxxxxxx"
      },
      "consignee": {
        "type": "string",
        "minLength": 1
      },
      "phone": {
        "type": "string",
        "minLength": 11,
        "maxLength": 11,
        "minLengthErrorCode": "minLength error",
        "maxLengthErrorCode": "maxLength error",
        "patternErrorCode": "pattern error"
      },
      "region": {
        "type": "string",
        "minLength": 1
      },
      "address": {
        "type": "string"
      }
    },
    "required": ["order_no", "amount", "consignee", "phone", "region"]
  }
}